AAA,KAFA;AAGA,MAAA,SAAA,EAAA,EAHA;AAIA,MAAA,OAAA,EAAA,EAJA;AAKA,MAAA,KAAA,EAAA,CALA;AAMA,MAAA,QAAA,EAAA,CANA;AAOA,MAAA,SAAA,EAAA,CAAA,CAAA,EAAA,EAAA,EAAA,EAAA,CAPA;AAQA,MAAA,WAAA,EAAA;AARA,KAAA;AAUA,GArCA;;AAsCA,EAAA,YAAA,GAAA;AACA,UAAA,MAAA,GAAA;AACA,MAAA,GAAA,EAAA,UADA;AAEA,MAAA,MAAA,EAAA;AAFA,KAAA;AAIA,IAAA,OAAA,CAAA,MAAA,CAAA,CAAA,IAAA,CAAA,IAAA,IAAA;AACA,UAAA,IAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,aAAA,OAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,aAAA,SAAA,GAAA,KAAA,OAAA;AACA,aAAA,KAAA,GAAA,KAAA,SAAA,CAAA,MAAA;AACA,OAJA,MAIA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA,CAAA;AACA;AACA,KARA,EAQA,KARA,CAQA,MAAA;AACA,MAAA,KAAA,CAAA,SAAA,CAAA;AACA,KAVA;AAWA;;AAtDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-dialog  :visible.sync=\"noticeVisible\" >\r\n      <el-form ref=\"editForm\"  label-width=\"100px\" class=\"demo-ruleForm\">\r\n        <el-form-item   :label-width=\"'10'\"  style=\"width: 25%;margin-left: 35%\"  >\r\n          <el-input v-model=\"notice.author\" type=\"text\"  autocomplete=\"off\" placeholder=\"发布者\" disabled></el-input>\r\n        </el-form-item>\r\n        <el-form-item  prop=\"title\" :label-width=\"'10'\"  style=\"width: 25%;margin-left: 35%\"  >\r\n          <el-input v-model=\"notice.title\" type=\"text\"  readonly autocomplete=\"off\" placeholder=\"公告标题\" ></el-input>\r\n        </el-form-item>\r\n        <el-form-item  prop=\"content\" :label-width=\"'10'\"  style=\"width: 50%;margin-left: 25%\"  >\r\n          <el-input v-model=\"notice.content\"  type=\"textarea\"  readonly  autosize autocomplete=\"off\" placeholder=\"公告内容\" ></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n    <h1 style=\"margin-top: 25px\">案例库公告</h1>\r\n    <el-table  :data.sync=\"tableData.slice((currentPage-1)*pagesize,currentPage*pagesize)\" style=\"width: 682px;margin-left: 30%;margin-top: 45px\" border stripe>\r\n      <el-table-column prop=\"title\"\r\n                       label=\"标题\" width=\"250\"\r\n                       header-align=\"center\"\r\n                       align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link  @click=\"getNotice(scope.row.id)\">{{ scope.row.title }}</el-link >\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"createTime\"\r\n                       label=\"创建时间\"\r\n                       width=\"180\"\r\n                       header-align=\"center\"\r\n                       align=\"center\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"author\"\r\n                       label=\"发布者\"\r\n                       width=\"250\"\r\n                       header-align=\"center\"\r\n                       align=\"center\">\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"text-align: center;margin-top: 30px;margin-right: 16%\">\r\n      <el-pagination\r\n          background\r\n          :page-size.sync=\"pagesize\"\r\n          :page-sizes.sync=\"pagesizes\"\r\n          layout=\"sizes, prev, pager, next, jumper, ->, total\"\r\n          :total.sync=\"total\"\r\n          @current-change=\"current_change\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getNotices,getNotice} from \"@/api/api\";\r\nimport {request} from \"@/network/request\";\r\n\r\nexport default {\r\nname: \"Notice\",\r\n  methods:{\r\n    getNotice(id){\r\n      const config={\r\n        url:getNotice,\r\n        method:\"get\",\r\n        params:{\r\n          \"id\":id\r\n        }\r\n      }\r\n      request(config).then((data)=>{\r\n        if(data.data.code===0){\r\n          this.notice=data.data.data\r\n          this.noticeVisible=true\r\n        }else {\r\n          alert(data.data.message)\r\n        }\r\n      }).catch(()=>{\r\n        alert(\"服务器开小差了\")\r\n      })\r\n    },\r\n    current_change:function(currentPage){\r\n      this.currentPage = currentPage;\r\n    }\r\n  },\r\n  data(){\r\n    return {\r\n      notice:{},\r\n      noticeVisible:false,\r\n      tableData: [],\r\n      allData: [],\r\n      total: 0,\r\n      pagesize:7,\r\n      pagesizes:[7,16,20],\r\n      currentPage:1\r\n    }\r\n  },\r\n  beforeCreate() {\r\n    const config={\r\n      url: getNotices,\r\n      method: 'get',\r\n    }\r\n    request(config).then((data)=>{\r\n      if(data.data.code===0){\r\n        this.allData=data.data.data\r\n        this.tableData=this.allData\r\n        this.total=this.tableData.length\r\n      }else{\r\n        alert(data.data.message)\r\n      }\r\n    }).catch(()=>{\r\n      alert(\"服务器开小差了\")\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}